{
  "name": "dart-flutter-utils",
  "displayName": "Dart/Flutter Utils",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dart-flutter-utils.dartNewClass",
    "onCommand:dart-flutter-utils.dartNewEnum"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-flutter-utils.dartNewClass",
        "title": "Dart: New Class"
      },
      {
        "command": "dart-flutter-utils.dartNewEnum",
        "title": "Dart: New Enum"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "dart-flutter-utils.dartNewClass",
          "group": "dart-flutter-utilsGroup@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "dart-flutter-utils.dartNewEnum",
          "group": "dart-flutter-utilsGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "lodash": "^4.17.21"
  }
}
